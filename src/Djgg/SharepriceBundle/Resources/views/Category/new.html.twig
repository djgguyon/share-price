{% extends "DjggSharepriceBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - new category{% endblock %}

{% block DjggShareprice_body %}

<p class="titre"><span>New category</span>
<a class="action_list" href="{{ path('djgg_shareprice_category_grid') }}">Back to the list</a>
</p> 

<form action="{{ path('djgg_shareprice_category_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

{% endblock %}